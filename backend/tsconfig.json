{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "typeRoots": ["./node_modules/@types", "./src/types"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }